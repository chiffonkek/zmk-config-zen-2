        tdds: Dot-Slash {
                compatible = "zmk,behavior-tap-dance";
                label = "Tap Dance Dot Slash";
                #binding-cells = <0>;
                tapping-term-ms = <170>;
                bindings = <&as LS(DOT) DOT>, <&as LS(FSLH) FSLH>;
